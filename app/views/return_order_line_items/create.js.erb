<% @rolis.each do |roli| %>
$("#return_order_line_items tr:first-child").after("<%= escape_javascript(render :partial=>'return_order_line_item',:object => roli) %>")
$("#return_order_line_item_<%= roli.id %>").effect("highlight");
<% end %>
$("#item").val("");
$("#return_order_meta").html("<%= escape_javascript(render :partial=>"return_orders/meta",:locals=>{:return_order=>@return_order_line_item.return_order}) %>");



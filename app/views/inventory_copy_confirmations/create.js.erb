$("#inventory_copy_confirmations tr:first-child").after("<%= escape_javascript(render :partial=>"inventory_copy_confirmation",:object => @inventory_copy_confirmation) %>");
$("#item").val("");
$("#item").autocomplete( "option", "minLength",5);
$("#item").autocomplete( "option", "delay",300);
$("#inventory_copy_confirmation_<%= @inventory_copy_confirmation.id %> .switch").bootstrapSwitch();
